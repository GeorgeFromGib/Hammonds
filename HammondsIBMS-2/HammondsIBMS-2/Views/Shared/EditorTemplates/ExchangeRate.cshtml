@model int
           
@{Html.Telerik().DropDownList()
        .Name(ViewData.TemplateInfo.GetFullHtmlFieldName(string.Empty))
        .ClientEvents(events => events.OnChange("DropdownChange"))
        .BindTo(new SelectList((System.Collections.IEnumerable)ViewBag.ExchangeRates, "ExchangeRateId", "AbbreviationSymbolComb", Model.ToString())).Render();}
